{
  "OpenAI": {
    "ApiKey": "your_openai_api_key"
  },
  "Claude": {
    "ApiKey": "your_claude_api_key",
    "ApiVersion": "2023-06-01"
  },
  "Redis": {
    "ConnectionString": "your-azure-redis-cache.redis.cache.windows.net:6380,password=your_redis_access_key,ssl=True,abortConnect=False"
