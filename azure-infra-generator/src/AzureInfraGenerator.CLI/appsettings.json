    "MaxTokens": 2000,
    "AIProvider": "OpenAI",
    "PreferredModel": "GPT_4"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  }
}